  },
  "dependencies": {
    "request": "*"
  },
  "devDependencies": {
    "rainbird-linter": "*",
    "jshint": "*",
    "mocha": "*",
    "chai": "*",
    "rewire": "*",
    "docco": "*",
    "istanbul": "*"
  }
}
